{
  "name": "Click to Dial",
  "description": "Click a number to call",
  "version": "0.1.0",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3,
  "icons": {
    "48": "icons/ctd-96.png",
    "128": "icons/ctd-144.png"
  }
}
