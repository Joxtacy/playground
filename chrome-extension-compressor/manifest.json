{
    "manifest_version": 3,
    "name": "Audio effects!",
    "description": "Add audio effects to YouTube videos!",
    "version": "0.0.1",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./images/get_started16.png",
            "32": "./images/get_started32.png",
            "48": "./images/get_started48.png",
            "128": "./images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}