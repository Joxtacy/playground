fn mul a b:
  a * b

fn add a b:
  a + b

export fn main argv:
  "Hello Derp" => greeting
  greeting -> console.log

  add 5 6 -> console.log
  2 -> add 69 -> console.log

  add () -> console.log

  add 2 3
    -> mul 4
    -> console.log

  [1 3 5] => arr
  arr -> console.log
  arr
    -> * mul
    -> * add 2
    -> console.log
